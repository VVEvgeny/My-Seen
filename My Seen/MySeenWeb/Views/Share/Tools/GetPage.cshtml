<script language="javascript" type="text/javascript">

    var sharedKey = '';
    var sharedPageName = '';

    function getPage(data, noReset) {

        //console.log("getPage data=" + data + " noReset=" + noReset);

        if (window.model) window.model.setLoadingData();

        var addData = { key: sharedKey, pageName: sharedPageName };
        if (data) {
            $.extend(addData, data);
        }
        data = addData;

        if (window.modelLayout) window.modelLayout.reload();
        //console.log("GetSharedPage data=", data);

        if (window.model && noReset !== true) {
            window.model.resetData();
        }

        $.post('/Json/GetSharedPage/', data, function(jsonData) {

            //console.log("GetSharedPage jsonData=", jsonData);

            window.showPage(jsonData);

        }, "json");
    };
</script>